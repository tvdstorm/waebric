ometa WaebricInterpreter {
  interp = ['module-id' anything:x]  -> '<id>'.concat(x).concat('</id>')
         | ['def' anything:x]  -> '<functiondef/>'
         | ['module' interp:x interp:y]  -> '<module>'.concat(x).concat(y).concat('</module>')
         | ['modules' [interp*:b]]  -> '<modules>'.concat(b.join('')).concat('</modules>')
}